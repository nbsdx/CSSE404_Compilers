/* overflow machine */
class Main {
    public static void main (String[] args) {
        int last = 0;
        int i = last;
        while (true) {
            i = i + 100000;
            if (i < last) {
                // An overflow occurred! I wish we had strings
                System.out.println(last);
                System.out.println(i);
            }
            // Added required else
            else {
                i = i;
            }
            last = i;
        }
    }
}
